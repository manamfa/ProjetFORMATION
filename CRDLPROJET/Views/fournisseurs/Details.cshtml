@model CRDLPROJET.Models.fournisseur

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Layouts/_Default.cshtml";
}
@Html.Raw(TempData["msg"])
<div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
    <div class="col-lg-12" style="background-color:aquamarine">
        <div class="panel panel-default" style="background-color:aquamarine">
            @*<div class="panel-heading" style="background-color:aquamarine"></div>*@
            <div class="panel-body" style="background-color:aquamarine">
                <div class="col-md-6" style="background-color:aquamarine">
                    <div>
                        <h4 style="text-align:center">Fournisseur</h4>
                        <hr />
                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.nomfournisseur)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.nomfournisseur)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.prenomfournisseur)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.prenomfournisseur)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.tel)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.tel)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.adresse)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.adresse)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.datent)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.datent)
                            </dd>

                        </dl>
                    </div>
                    <p class="btn btn-warning">
                        @Html.ActionLink("Edit", "Edit", new { id = Model.fournisseurID }) |
                        @Html.ActionLink("Lister Fournisseur", "Index")
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
